{
  "name": "@elebase/sdk",
  "version": "1.1.1",
  "description": "Elebase JavaScript Development Kit",
  "author": "Elebase <hello@elebase.io> (https://elebase.io)",
  "homepage": "https://github.com/elebase/js-sdk",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elebase/js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/elebase/js-sdk/issues"
  },
  "keywords": [
    "elebase",
    "geo",
    "api"
  ],
  "browserslist": [
    "> 2%",
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              ">0.25%",
              "not ie 11",
              "not op_mini all"
            ],
            "node": "10.0"
          }
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "main": "./dist/elebase.node.js",
  "browser": "./dist/elebase.js",
  "scripts": {
    "build": "BABEL_ENV=webpack NODE_ENV=production webpack"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "babel-runtime": "^6.26.0",
    "jshashes": "^1.0.8",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "webpack": "^3.12.0"
  }
}
